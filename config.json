{
  "dataPath": "https://data.aachen.freifunk.net/",
  "siteName": "Freifunk Regio Aachen",
  "mapSigmaScale": 0.5,
  "showContact": false,
  "maxAge": 14,
  "mapLayers": [
    {
      "name": "OpenStreetMap",
      "url": "https:///tiles.aachen.freifunk.net/{z}/{x}/{y}.png",
      "config": {
        "type": "osm",
        "attribution": "&copy; <a href=\"https://www.openstreetmap.org/copyright\" target=\"_blank\">OpenStreetMap-Mitwirkende</a>",
        "maxZoom": 19
      }
    },
    {
      "name": "Stamen.TonerLite"
    }
  ],
  "siteNames": [
    { "site": "ffac", "name": "Aachen" }
  ],
  "nodeCharts": [
    {
      "name": "Statistik",
      "metrics": [
        {
          "id": "clientcount",
          "color": "#1566A9",
          "label": "Clients"
        },
        {
          "id": "loadavg",
          "color": "#1566A9",
          "label": "Load"
        },
        {
          "id": "uptime",
          "color": "#1566A9",
          "label": "Uptime"
        }
      ],
      "defaultMetric": "clientcount",
      "data": {
        "url": "https://graphite.ffac.aachalon.net/json/###ZOOM_FROM###/###NODE_ID###",
        "parameters": [
        ]
      }
    }
  ]
}
